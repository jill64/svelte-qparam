{
  "name": "svelte-qparam",
  "description": "ðŸ”Ž Type-Safe Query Parameter for SvelteKit",
  "version": "0.2.1",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/svelte-qparam/issues",
  "homepage": "https://github.com/jill64/svelte-qparam#readme",
  "files": [
    "dist",
    "!**/*.test.*"
  ],
  "exports": {
    "./serde": {
      "types": "./dist/serde/index.d.ts",
      "default": "./dist/serde/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-qparam.git"
  },
  "keywords": [
    "querystring",
    "svelte",
    "sveltekit",
    "typesafe"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "npm run build && vite preview",
    "prepack": "npm run build",
    "package": "svelte-kit sync && npx @sveltejs/package && npx publint",
    "check": "svelte-kit sync && npx svelte-check",
    "lint": "npm run check && npx eslint .",
    "format": "npx @jill64/psx",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0",
    "svelte": "^4.0.0"
  },
  "prettier": "@jill64/prettier-config",
  "devDependencies": {
    "@playwright/test": "1.40.1",
    "@sveltejs/adapter-auto": "2.1.1",
    "@sveltejs/kit": "1.27.7",
    "@jill64/eslint-config-svelte": "1.0.0",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/playwright-config": "1.0.0",
    "svelte": "4.2.8",
    "typescript": "5.3.2",
    "vite": "5.0.5",
    "vitest": "1.0.1"
  },
  "dependencies": {
    "typed-qparam": "2.1.2"
  }
}
