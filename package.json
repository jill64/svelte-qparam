    "@jill64/playwright-config": "3.0.0",
    "@jill64/prettier-config": "1.0.0",
    "@jill64/sentry-sveltekit-cloudflare": "2.0.6",
    "@playwright/test": "1.51.0",
    "@sveltejs/adapter-cloudflare": "7.0.1",
    "@sveltejs/kit": "2.20.6",
    "svelte": "5.23.2",
    "svelte-page-tab": "2.0.9",
